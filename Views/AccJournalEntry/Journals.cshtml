<div id="nonPrintArea" class="container p-3">
    <div class="card">
        <div class="card-header"><b>Journal Entry List </b></div>
        <div class="card-body table-responsive">
            <span id="app_err-container"></span>
            <table class="table table-sm table-hover table-bordered">
                <thead class="dark-mode">
                    <tr>
                        <th class="text-center" scope="col">Date</th>
                        <th class="text-center" scope="col">Type</th>
                        <th class="text-center" scope="col">Trans</th>
                        <th class="text-center" scope="col">Reference</th>
                        <th class="text-center" scope="col">Amount</th>
                        <th class="text-center" scope="col">Memo</th>
                        <th class="text-center" scope="col">User</th>
                        <th class="text-center" scope="col">View</th>
                        <th class="text-center" scope="col"></th>
                    </tr>
                </thead>
                <tbody id="tJournalListbody" class="txt-align-left">
                </tbody>
            </table>
        </div>
     </div>
</div>
@section Scripts{
    <script src="~/js/accJournalEntry.js"></script>
    <script>
        $(document).ready(function () {
            getAllJournalEntry();
        });
    </script>
}